@model Model.RepositoryModels.RepositoryModelTicket

@{
    ViewBag.Title = "Billett";
    Layout = "_Layout";
}

@Html.AntiForgeryToken()
<div class="ticket">
    <div>
        <label>Reise: </label>
        @Html.DisplayFor(model => model.FromStation) - @Html.DisplayFor(model => model.ToStation) <br/>

        <label>Gyldig Fra: </label>
        @Html.DisplayFor(model => model.ValidFromDate) @Html.DisplayFor(model => model.ValidFromTime) <br/>

        <label>Pris: </label>
        @Html.DisplayFor(model => model.Price),- <br/>

        <label>Navn: </label>
        @Html.DisplayFor(model => model.CustomerGivenName) @Html.DisplayFor(model => model.CustomerLastName) <br/>

        <label>Tlf Nummer: </label>
        @Html.DisplayFor(model => model.CustomerNumber)
    </div>
</div>